<body>

    <!-- Preloader Start -->
    <div id="preloader-active">
        <div class="preloader d-flex align-items-center justify-content-center">
            <div class="preloader-inner position-relative">
                <div class="preloader-circle"></div>
                <div class="preloader-img pere-text">
                    <img src="~/Content/assets/img/logo/loder.png" alt="">
                </div>
            </div>
        </div>
    </div>
    <!-- Preloader Start -->

    <header>
        <!-- Header Start -->
        <div class="header-area header-sticky">
            <div class="container-fluid">
                <div class="row align-items-center" style="padding: 15px 0px;">
                    <!-- Logo -->
                    <div class="col-xl-3 col-lg-2 col-md-2">
                        <div class="d-flex align-items-center">
                            <div class="logo w-100">
                                <a class="d-block" href="@Url.Action("Index","Home")">
                                  <img  src="~/Content/assets/img/logo/logo.png" alt=""></a>
                            </div>
                            <div class="text-light logoText" style="margin-left: 10px;">
                                Invoice Tool
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-7 col-lg-8 col-md-8">
                        <!-- Main-menu -->
                        <div class="main-menu f-right d-none d-lg-block">
                            <nav>
                                <ul id="navigation">
                                    @*<li><a href="@Url.Action("Index","Home")">Home</a></li>
                                    <li><a href="@Url.Action("Index","ImportNewFile")">Upload New File</a></li>
                                    <li><a href="@Url.Action("FailedProducts","SubmitProducts")">Failed Products</a></li>*@
                                    @if (((ClaimsIdentity) User.Identity).Claims.FirstOrDefault(s => s.Value == "Admin") != null)
                                    {
                                        <li><a href="/admin">Admin</a></li>
                                    }
                                </ul>
                            </nav>
                        </div>
                    </div>
                    <div class="col-xl-2 col-lg-2 col-md-2">
                        <!-- Header-btn -->
                        <div class="d-none d-lg-block f-right">
                            <a href="#" class="genric-btn danger-border circle">@User.Identity.Name.Remove(0, 11)</a>
                        </div>
                    </div>
                    <!-- Mobile Menu -->
                    <div class="col-12">
                        <div class="mobile_menu d-block d-lg-none"></div>
                    </div>
                </div>
            </div>
        </div>
        <div id="ProgressBar" style="display:none"></div>
        <!-- Header End -->
    </header>
    <main>
        @RenderBody()
    </main>
    <footer>

        <div class="footer-bottom-area footer-bg">
            <div class="container">
                <div class="footer-border">
                    <div class="row d-flex align-items-center">
